{
  "name": "tampermonkey-scripts",
  "version": "1.0.0",
  "description": "Collection of Tampermonkey userscripts and browser automation tools",
  "private": true,
  "workspaces": [
    "llm-judge",
    "*/desktop-app"
  ],
  "scripts": {
    "install-all": "npm install && npm run install-aws-launcher",
    "install-aws-launcher": "cd aws-role-launcher/desktop-app && npm install",
    "start-aws-launcher": "cd aws-role-launcher/desktop-app && npm start",
    "build-aws-launcher": "cd aws-role-launcher/desktop-app && npm run dist",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "lint": "echo 'Add linting configuration as needed'",
    "test": "echo 'Add testing configuration as needed'"
  },
  "keywords": [
    "tampermonkey",
    "userscripts",
    "browser-automation",
    "aws",
    "productivity"
  ],
  "author": "iddv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iddv/tampermonkey.git"
  },
  "devDependencies": {
    "eslint": "^8.0.0"
  }
}